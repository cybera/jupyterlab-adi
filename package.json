{
  "name": "adi",
  "version": "0.1.0",
  "description": "A JupyterLab extension to interact with the ADI platform",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/cybera/jupyterlab-adi",
  "bugs": {
    "url": "https://github.com/cybera/jupyterlab-adi/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Cybera",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/cybera/jupyterlab-adi.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@jupyterlab/application": "^1.1.3",
    "@jupyterlab/apputils": "^1.1.3",
    "@jupyterlab/notebook": "^1.1.3",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "@material-ui/styles": "^4.6.0",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/messaging": "^1.3.0",
    "@phosphor/widgets": "^1.9.2",
    "apollo-boost": "^0.4.4",
    "filbert": "^0.1.20",
    "graphql": "^14.5.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6",
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "resolutions": {
    "**/@types/react": "^16.8.6"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
